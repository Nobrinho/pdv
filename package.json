{
  "name": "syscontrol-desktop",
  "version": "1.0.0",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "electron .",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "migrate:make": "knex migrate:make --knexfile ./database/knexfile.js",
    "migrate:latest": "knex migrate:latest --knexfile ./database/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile ./database/knexfile.js"
  },
  "dependencies": {
    "chart.js": "^4.4.1",
    "dayjs": "^1.11.10",
    "knex": "^3.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^39.2.6",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "vite": "^7.2.6",
    "wait-on": "^7.2.0"
  }
}
